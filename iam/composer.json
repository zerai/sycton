{
    "name": "zerai/sycton-iam",
    "type": "project",
    "description": "funny stuff",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-openswoole": "*",
        "composer/package-versions-deprecated": "1.11.99.*",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "ecotone/amqp": "^1.94",
        "ecotone/dbal": "^1.90",
        "ecotone/jms-converter": "^1.94",
        "ecotone/pdo-event-sourcing": "^1.90",
        "ecotone/symfony-bundle": "^1.90",
        "league/oauth2-server-bundle": "^0.4.1",
        "symfony/asset": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/flex": "^1.17 || ^2",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-kernel": "6.2.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/proxy-manager-bridge": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/yaml": "6.2.*",
        "zerai/openswoole-runtime": "^0.1"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": false,
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "IdentityAccess\\": "_iam/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "IdentityAccess\\Tests\\": "_iam/tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "cs": "./vendor/bin/ecs",
        "cs:fix": "./vendor/bin/ecs --fix",
        "phpstan": "./vendor/bin/phpstan",
        "psalm": "./vendor/bin/psalm",
        "rc": "./vendor/bin/rector --dry-run",
        "rc:fix": "./vendor/bin/rector",
        "tf": "./bin/phpunit --testsuite=functional --testdox",
        "ti": "./bin/phpunit --testsuite=integration --testdox",
        "tu": "./bin/phpunit --testsuite=unit --testdox"
    }
}
