{
    "name": "opendaje/symfony-es-cqrs-boilerplate",
    "type": "project",
    "description": "symfony es cqrs boilerplate",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.*",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "ecotone/amqp": "^1.8",
        "ecotone/dbal": "^1.8",
        "ecotone/jms-converter": "^1.8",
        "ecotone/pdo-event-sourcing": "^1.8",
        "ecotone/symfony-bundle": "^1.8",
        "runtime/swoole": "^0.3.1",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.17 || ^2",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-kernel": "5.4.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/yaml": "5.4.*"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "dama/doctrine-test-bundle": "^7.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": false,
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "IdentityAccess\\": "srcIam/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "IdentityAccess\\Tests\\": "srcIam/tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
