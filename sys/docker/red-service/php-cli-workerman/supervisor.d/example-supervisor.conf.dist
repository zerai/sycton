#umask=022
#priority=999
#autostart=true
#autorestart=unexpected
#startsecs=10
#startretries=3
#exitcodes=0
#stopsignal=TERM
#stopwaitsecs=10
#stopasgroup=false
#killasgroup=false
#user=chrism
#redirect_stderr=false
#stdout_logfile=/a/path
#stdout_logfile_maxbytes=1MB
