{
    "name": "zerai/sycton-customer",
    "type": "project",
    "description": "funny stuff",
    "license": "MIT",
    "require": {
        "php": ">=8.0 || ^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "ecotone/amqp": "^1.8",
        "ecotone/jms-converter": "^1.8",
        "ecotone/pdo-event-sourcing": "^1.8",
        "ecotone/symfony-bundle": "^1.5",
        "runtime/swoole": "^0.3.1",
        "symfony/console": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.2.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/proxy-manager-bridge": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/yaml": "6.2.*",
        "twig/extra-bundle": "^2.12 || ^3.0",
        "twig/twig": "^2.12 || ^3.0",
        "zerai/openswoole-runtime": "^0.1.0",
        "zerai/workerman-symfony-runtime": "@dev"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "dama/doctrine-test-bundle": "^7.1",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": false,
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Customer\\": "_customer/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Customer\\Tests\\": "_customer/tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zerai/workerman-symfony-runtime.git"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "cs": "./vendor/bin/ecs",
        "cs:fix": "./vendor/bin/ecs --fix",
        "psalm": "./vendor/bin/psalm",
        "rc": "./vendor/bin/rector --dry-run",
        "rc:fix": "./vendor/bin/rector",
        "tf": "./bin/phpunit --testsuite=functional --testdox",
        "ti": "./bin/phpunit --testsuite=integration --testdox",
        "tu": "./bin/phpunit --testsuite=unit --testdox"
    }
}
